{
  "functions": {
    "source": "." 
  },
  "hosting": {
    "public": "dist/combined",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "api"
      },
      {
        "source": "/manager/**",
        "destination": "/manager/index.html"
      }
    ]
  }
}